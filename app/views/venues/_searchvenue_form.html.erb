<%= semantic_form_for(@searchvenue) do |f| %>

	<%= f.inputs do %>
		<%= f.input :location %>
		<%= f.input :venue_category, :as => :select, :collection => ["Sports Field - Soccer", "Sports Field - General", "Hall -Church", "Hall - Community", "Hall - Other"] %>
		<%= f.input :venue_size, :as => :select, :collection => ["Small", "Medium", "Large"] %>
	<% end %>
	<%= f.buttons do %>
		<%= f.commit_button "Search Venues" %>
	<% end %>

<% end %>